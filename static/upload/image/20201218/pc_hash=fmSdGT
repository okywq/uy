<script type="text/javascript">
$('#game_y a').live("click",function(){
$(".nr .on").attr('class','nr-item');
$('#search_word').val('');
var goy = $(this).attr("data");
var y = $("#game_y").attr("data");
var m = $("#game_m").attr("data");
var keywords=$('#search_word').val();
if(goy!=y){
var url = "/api.php?op=special&action=get_ymdatas&y="+goy+"&m="+m+"&keywords="+keywords+"&specialid=932&typeid=2258";
$("#show_special").html("");
$('#load_more').remove();
loadpos(url);
$('#game_y a').removeClass("on");
$(this).addClass("on");
$("#game_y").attr("data",goy);
}

})

$('#game_m a').live("click",function(){
$(".nr .on").attr('class','nr-item');
$('#search_word').val('');
var gom = $(this).attr("data");
var y = $("#game_y").attr("data");
var m = $("#game_m").attr("data");
var keywords=$('#search_word').val();
if(gom!=m){
var url = "/api.php?op=special&action=get_ymdatas&y="+y+"&m="+gom+"&keywords="+keywords+"&specialid=932&typeid=2258";
$("#show_special").html("");
	$('#load_more').remove();
loadpos(url);
$('#game_m a').removeClass("on");
$(this).addClass("on");
$("#game_m").attr("data",gom);
}

})

$(function(){
document.onkeydown = function(e){ 
    var ev = document.all ? window.event : e;
    if(ev.keyCode==13) {

		$(".nr .on").attr('class','nr-item');
		var keywords=$('#search_word').val();

		var url = "/api.php?op=special&action=get_ymdatas&keywords="+keywords+"&specialid=932&typeid=2258";
		$("#show_special").html("");
		$('#load_more').remove();
		loadpos(url);

     }
}
});

$('#dj').live("click",function(){

	$(".nr .on").attr('class','nr-item');
	var keywords=$('#search_word').val();

	var url = "/api.php?op=special&action=get_ymdatas&keywords="+keywords+"&specialid=932&typeid=2258";
	$("#show_special").html("");
	$('#load_more').remove();
	loadpos(url);


})

$('#nr .nr-item').on('click', function(){
    $('#search_word').val('');
	var t = $(this);
	var	da = t.data('url');
	var	url = "/api.php?op=special&action=get_ymdatas&nr_hdlx="+da+"&specialid=932&typeid=2258";
		
	t.addClass('on').siblings().removeClass('on');
	$("#show_special").html("");
	$('#load_more').remove();
	loadpos(url);
})


$('#load_more a').live("click",function(){
var y = $("#game_y").attr("data");
var m = $("#game_m").attr("data");
var	da = $(".nr .on").data('url');
var page = $("#load_more").attr("data");
var keywords=$('#search_word').val();

if(keywords){
var url = "/api.php?op=special&action=get_ymdatas&keywords="+keywords+"&page="+page+"&specialid=932&typeid=2258";
}else if(da){
var url = "/api.php?op=special&action=get_ymdatas&nr_hdlx="+da+"&page="+page+"&specialid=932&typeid=2258";
}else{
var url = "/api.php?op=special&action=get_ymdatas&page="+page+"&specialid=932&typeid=2258";
}

page++;
loadpos(url,page);
})
	function loadpos(url,page){

	 $.ajax({

     type: "GET",

     url: url,

     dataType: "json",

     data: "",

     contentType: "application/json",

     success: function(data){
	$('#load_more').remove();
	  if(data){
 var i = show_data(data||[]);
	if(!page) page=2;
	if(i==10 && data[10]){
	var load="<div class='load_more' id='load_more' data='"+page+"'><a href='javascript:void(0)'>查看更多</a></div>	"
	$('.zt_lists').append(load);
	}

	  }

     },

     error: function(){

     }



    });

	}
function show_data(data){
for (var i = 0; i<10; i++) {
	show_data_list(data[i]);
	}
return i;
 }
function show_data_list(list){

var htm=[" <div class='nrh'>                                                                                                    ",
"    <a class='pic' href='",list.url,"' target='_blank'><img src='",list.thumb,"' width='200' height='120' alt='",list.title,"'></a>",
"    <div class='text_box'>                                                                               ",
"       <h5><a href='",list.url,"' target='_blank'>",list.title,"</a></h5>                               ",
"       <p class='text'>                                                                                  ",
list.description,
"       </p>                                                                                              ",
"       <p class='bottom'>                                                                                ",
"         <a href='",list.url,"' target='_blank'>阅读全文</a>                                                         ",
"         <span>",list.time,"</span>                                                                         ",
"       </p>                                                                                              ",
"    </div>                                                                                               ",
" </div>"].join('');
$("#show_special").append(htm);
}
